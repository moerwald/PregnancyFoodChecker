<UserControl x:Class="PregnancyFoodCheckWpf.Views.CalculatePregnancyWeek"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PregnancyFoodCheckWpf.Views"
             xmlns:vm="clr-namespace:PregnancyFoodCheckWpf.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <vm:CalculatePregnancyWeekViewModel x:Key="calcPregVm"/>
    </UserControl.Resources>
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Margin="5">Letzte Periode:</TextBlock>
            <DatePicker Margin="5" HorizontalAlignment="Left"></DatePicker>
            <TextBlock Margin="5">Aktuelle SSW:</TextBlock>
            <TextBlock Margin="5"></TextBlock>
        </StackPanel>

    </Grid>
</UserControl>
